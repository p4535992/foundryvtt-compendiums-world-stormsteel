"},"source":"XGE","quantity":1,"weight":4,"price":510,"attunement":0,"equipped":false,"rarity":"","identified":false,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":0,"units":""},"target":{"value":0,"width":null,"units":"","type":""},"range":{"value":5,"long":0,"units":"ft"},"uses":{"value":0,"max":0,"per":""},"consume":{"type":"","target":"","amount":null},"ability":"str","actionType":"mwak","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[["1d8+@mod","slashing"]],"versatile":"1d10+@mod"},"formula":"","save":{"ability":"","dc":0,"scaling":"spell"},"armor":{"value":10},"hp":{"value":0,"max":0,"dt":null,"conditions":""},"weaponType":"martialM","baseItem":"","properties":{"ver":true},"proficient":true,"attributes":{"spelldc":10}},"sort":1001,"flags":{"plutonium":{"page":"items.html","source":"XGE","hash":"adamantine%20battleaxe_xge"},"externalactor":{"labels":{"activation":"1 Action","target":"","range":"5 Feet","duration":"","recharge":"Recharge [undefined]","damage":"1d8+@mod","damageTypes":"Slashing","abilityCheck":"Strength Ability Check","toHit":"+ 4","derivedDamage":[{"formula":"1d8 + 2","damageType":"slashing"}]}}},"img":"icons/weapons/polearms/halberd-crescent-engraved-steel.webp","effects":[],"folder":null,"permission":{"default":0}},{"_id":"AZdRmBoYUOC3jdll","name":"Ivaldi","type":"spell","data":{"description":{"value":"<div class=\"rd__b  rd__b--3\">\n<p>You create a 20-foot-radius sphere of fire centered on a point within range. All enemies in an area you designate must make a Dexterity saving throw, being <a href=\"https://5e.tools/conditionsdiseases.html#blinded_phb\">blinded</a> and taking [[/r 10d6]] radiant damage on a failure, or half as much damage with no blind on a success.</p>\n<div class=\"rd__spc-inline-post\">&nbsp;</div>\n</div>\n<div class=\"rd__b  rd__b--3\">\n<div class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"1\"> <span class=\"entry-title-inner\">At Higher Levels.</span></span>\n<p>When you cast this spell using a spell slot of 6th level or higher, the damage increases by [[/r 1d6]] radiant, for each slot level above 5th.</p>\n<div class=\"rd__spc-inline-post\">&nbsp;</div>\n</div>\n<div class=\"rd__spc-inline-post\">&nbsp;</div>\n</div>","chat":"","unidentified":""},"source":"p4535992Spells","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":20,"width":null,"units":"ft","type":"sphere"},"range":{"value":100,"long":0,"units":"ft"},"uses":{"value":0,"max":"0","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[["10d6","radiant"]],"versatile":""},"formula":"","save":{"ability":"dex","dc":null,"scaling":"spell"},"level":5,"school":"evo","components":{"value":"","vocal":false,"somatic":true,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"prepared","prepared":true},"scaling":{"mode":"level","formula":"1d6"},"attributes":{"spelldc":10}},"sort":15001,"flags":{"core":{"sourceId":"Item.ZA3ceMkMritbZCOM"},"externalactor":{"labels":{"level":"5th Level","school":"Evocation","components":["S"],"materials":"","activation":"1 Action","target":"20 Feet Sphere","range":"100 Feet","duration":"Instantaneous","recharge":"Recharge [undefined]","damage":"10d6","damageTypes":"Radiant","save":"DC 12 Dexterity","derivedDamage":[{"formula":"10d6","damageType":"radiant"}],"abilityCheck":"undefined Ability Check"}}},"img":"modules/plutonium/media/icon/scroll-unfurled.svg","effects":[],"folder":null,"permission":{"default":0}}],"effects":[],"folder":null,"sort":0,"permission":{"default":0,"a2um1mRnicEx282I":3},"flags":{"inventory-plus":{"categorys":{"weapon":{"label":"DND5E.ItemTypeWeaponPl","dataset":{"type":"weapon"},"sortFlag":2000,"ignoreW